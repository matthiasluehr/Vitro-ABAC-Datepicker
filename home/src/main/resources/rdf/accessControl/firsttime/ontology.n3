ao:PolicyKey rdf:type owl:Class ;
     rdfs:label "Policy key"@en-US .

ao:Attribute rdf:type owl:Class ;
     rdfs:label "Access rule attribute"@en-US .
     
ao:Operator rdf:type owl:Class ;
